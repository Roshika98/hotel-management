<div class="col-xl-6 col-lg-12">
    <div class="card card-chart">
        <div class="card-header card-header-success">
            <div class="text-end pt-1">
                <p class="text-sm text-white mb-0 text-capitalize font-weight-bolder">Booking Details</p>
                <h4 class="mb-0"><%= details.user.name %> </h4>
            </div>
        </div>
        <div class="card-body">
            <h4 class="card-title">Package Type : <span class="text-info"><%= details.package.packageType %></span>
            </h4>
            <h5 class="card-title">Rooms : <span class="text-info"><%= details.roomCount %></span></h5>
            <h5 class="card-title">Check-in : <span class="text-info">
                    <%= details.checkIn.toDateString()+' '%></span> - Check-Out : <span class="text-info">
                    <%= details.checkOut.toDateString() %></span></h5>
            <!-- <p class="card-category">
                <span class="text-success"><i class="fa fa-long-arrow-up"></i> 55% </span> increase in today sales.
            </p> -->
        </div>
        <div class="card-footer">
            <!-- <div class="stats">
                <i class="material-icons">access_time</i> updated 4 minutes ago
            </div> -->
            <button type="button" class="btn btn-warning itemCart" data-itemCode="<%= details.id %>"
                id="processBooking">Process
                Check-in</button>
        </div>
    </div>
</div>