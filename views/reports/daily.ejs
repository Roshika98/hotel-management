    <body>
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr>
                <td align="left" bgcolor="#ffe77b" style="background-color: #fff;">
                    <h2 style="font-weight: 900;color: brown;">Rainbow Beach Hotel</h2>
                    <p>No. 03, Carron Place, Kudapaduwa, Negombo, Sri Lanka<br />
                        http://dinostore.bed | joy@rainbowbeachhotel.com +94 312 222 082</p>
                </td>
            </tr>
        </table>
        <h1 style="font-size: 20px;font-weight: 600; color: green; margin-top: 10px; text-align: center;">Daily
            Reservations Report</h1>
        <table width="95%" border="0" cellspacing="0" cellpadding="0">
            <tr>
                <!-- <td width="30%" align="left" valign="top" bgcolor="#ffe77b" style="background-color:#fff;">
                    <p>+152 5248 523</p>
                    <p>info@gmail.com</p>
                </td> -->
                <td width="40%" align="left" bgcolor="#ffe77b" style="background-color: #fff;">
                </td>
                <td width="30%" align="right" bgcolor="#ffe77b" style="background-color: #fff;">
                    <% var newDate=new Date() %>
                    <h6 style="font-weight: 700;color: #000000;">Date: <%= newDate.toDateString() %></h6>
                    <!-- <h4 style="font-weight: 700;color: #000000;">INVOICE NO: DX-5236</h4> -->
                </td>
            </tr>
        </table>
        <table class="table" width="95%">
            <!-- <thead>
                <tr>
